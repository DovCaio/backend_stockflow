generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Product {
  id Int @default(autoincrement()) @id
  nome String
  SKU String
  qtt Int
  qttMin Int 
  historics Historic[]
}

model Historic {
  id Int @default(autoincrement()) @id
  log String
  qttMin Int
  creatAt DateTime @default(now())
  product Product @relation(fields: [productId], references: [id])
  productId Int

}